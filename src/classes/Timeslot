package classes;

public enum Timeslot {
    MORNING(8, 0),
    AFTERNOON(12, 0),
    EVENING(18, 0);

    private final int hour;
    private final int minute;

    Timeslot(int hour, int minute) {
        this.hour = hour;
        this.minute = minute;
    }

    public int getHour() {
        return hour;
    }

    public int getMinute() {
        return minute;
    }
}
